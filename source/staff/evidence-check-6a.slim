javascript:
  $(document).ready( function() {
    $('#income').text('£' + getUrlParameter('income'));
    switch (getUrlParameter('outcome')) {
      case 'none':
        $('#income-result-display')
          .removeClass('summary-result-success')
          .addClass('summary-result-failure')
          .text('✗ Failed');
        $('#remission-type').addClass('callout-none');
        $('#remission-type h3').text('The applicant must pay the full fee');
        buildNextUrl('evidence-check-7b.html');
        break;
      case 'part':
        to_pay = getUrlParameter('to_pay') || '0'
        $('#remission-type').addClass('callout-part');
      $('#remission-type h3').text('The applicant must pay ' + unescape(to_pay) + ' towards the fee');
        buildNextUrl('evidence-check-7c.html');
        break;
      default:
        $('#remission-type').addClass('callout-full');
        //do nothing to the text, full is the default
    }
  });
